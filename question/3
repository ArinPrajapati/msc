#include <algorithm>
#include <iostream>
#include <utility>
#include <vector>

struct Job {
  char id;
  int deadline;
  int profit;
};

using namespace std;

bool compare(Job a, Job b) { return a.profit > b.profit; }
// [{a,3,100}]
pair<vector<char>, int> jobSequecing(Job jobs[], int n) {
    sort(jobs, jobs + n, compare);


